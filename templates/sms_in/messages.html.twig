{% extends 'base.html.twig' %}

{% block title %}Hello {{ controller_name }}!{% endblock %}
<h2>Received Messages</h2>
<table border="1">
<tr>
	<td>Group</td>
	<td>Sender</td>
	<td>Message</td>
</tr>
{% for message in messages %}
	<tr>
		<td>{{message.grp}}</td>
		<td>{{message.smsOrigin}}</td>
		<td>{{message.message}}</td>
	</tr>
{% endfor %}
</table>